record(stringout, "$(P):version")
{
   field(VAL, "0.1")
}

record(calc, "$(P):counter")
{
    field(SCAN, "1 second")
    field(CALC, "(A<B)?(A+C):D")
    field(INPA, "$(P):counter")
    field(INPB, "9")
    field(INPC, "1")
    field(INPD, "0")
    field(EGU, "Counts")
}

record(ao, "$(P):delay")
{
    field(DTYP, "Demo Neutron Delay")
    field(DOL,  "1.00")
    field(PREC, "2")
    field(EGU,  "seconds")
    field(PINI, "YES")
}

record(ao, "$(P):count")
{
    field(DTYP, "Demo Neutron Count")
    field(DOL,  "10")
    field(EGU,  "events")
    field(PINI, "YES")
}
 
record(ao, "$(P):id")
{
    field(DTYP, "Demo Neutron ID")
    field(DOL,  "0")
    #field(EGU,  "ID")
    field(PINI, "YES")
}
       
